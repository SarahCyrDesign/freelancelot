<ul>
        <% if !current_user %>
            <li><%= link_to "Sign up", new_user_registration_path %></li>
            <li><%= link_to "Log in", new_user_session_path %></li>
        <% elsif current_user.client? %>
          <li>logged in as: <%= link_to current_user.email, edit_user_registration_path %></li>
        <% else %>
          <li>logged in as: <%= link_to current_user.email, edit_user_registration_path %></li>
          <li><%= link_to "My Job Applications", job_applications_path unless current_user.admin? %></li>
          <li><%= link_to "Log out", destroy_user_session_path, :method => 'delete' %></li>
        <% end %>
      </ul>
